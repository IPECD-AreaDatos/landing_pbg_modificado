{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/",
      "destination": "/pbg-dashboard",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/pbg-dashboard/(.*)",
      "destination": "/pbg-dashboard/$1"
    }
  ]
}